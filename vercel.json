{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/_astro/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "bond-llc-main.vercel.app"
        }
      ],
      "destination": "https://bond-llc.jp/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "bond-llc-main-*.vercel.app"
        }
      ],
      "destination": "https://bond-llc.jp/:path*",
      "permanent": true
    }
  ]
}