{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/_astro/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/tech/:path*",
      "destination": "https://media-site-pf3yyy09s-madao1056s-projects.vercel.app/tech/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "bond-llc-main-(.+).vercel.app"
        }
      ],
      "destination": "https://bond-llc.jp/:path*",
      "permanent": true
    }
  ]
}